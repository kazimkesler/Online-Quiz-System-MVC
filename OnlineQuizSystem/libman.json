{
  "version": "1.0",
  "defaultProvider": "cdnjs",
    "libraries": [

        {
            "provider": "cdnjs",
            "library": "jquery@3.6.0",
            "destination": "wwwroot/lib/jquery/"
        },
        {
            "provider": "cdnjs",
            "library": "jquery-validate@1.19.5",
            "destination": "wwwroot/lib/jquery-validate/"
        },
        {
            "provider": "cdnjs",
            "library": "jquery-validation-unobtrusive@4.0.0",
            "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
        },
        {
            "provider": "cdnjs",
            "library": "bootstrap-sweetalert@1.0.1",
            "destination": "wwwroot/lib/bootstrap-sweetalert/"
        },
        {
            "provider": "cdnjs",
            "library": "sweetalert@2.1.2",
            "destination": "wwwroot/sweetalert/"
        }
    ]
}